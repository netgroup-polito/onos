<drivers>
	<driver name="examplenetconfdriver-netconf" extends="netconf" manufacturer="Manufacturer"
		hwVersion="MyDevice">
		<behaviour api="org.onosproject.net.device.DeviceDescriptionDiscovery"
			impl="org.onosproject.drivers.examplenetconfdriver.MyDeviceDeviceDescription" />
		<behaviour api="org.onosproject.net.behaviour.ConfigGetter"
			impl="org.onosproject.drivers.examplenetconfdriver.NetconfConfigGetter" />
		<behaviour api="org.onosproject.net.behaviour.Pipeliner"
			impl="org.onosproject.drivers.examplenetconfdriver.MyDevicePipeliner" />
		<behaviour api="org.onosproject.net.flow.FlowRuleProgrammable"
			impl="org.onosproject.drivers.examplenetconfdriver.MyDeviceFlowRuleProgrammable" />
		<behaviour api="org.onosproject.net.behaviour.MeterQuery"
			impl="org.onosproject.drivers.examplenetconfdriver.FullMetersAvailable" />
        <!--		<behaviour api="org.onosproject.net.behaviour.InterfaceConfig"
                           impl="org.onosproject.drivers.cisco.InterfaceConfigTiesseImpl"/>  -->
        <behaviour api="org.onosproject.drivers.examplenetconfdriver.InterfaceConfigExtended"
                   impl="org.onosproject.drivers.examplenetconfdriver.InterfaceConfigTiesseImpl"/>
            </driver>
        </drivers>
